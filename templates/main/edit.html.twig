
    {% extends 'base.html.twig' %}

    {% block title %}Planning Type{% endblock %}

    {% block body %}
        <h2>Modifier une Plage Horaire et un etat a un employé connecté</h2>
        {{ form_row(plageHoraireForm.planningType.inom) }}
        {{ form_row(plageHoraireForm.num_jour) }}
        {{ form_row(plageHoraireForm.nom_plage) }}
        {{ form_row(plageHoraireForm.debut) }}
        {{ form_row(plageHoraireForm.fin) }}

        {{ form_row(plageHoraireForm.etats) }}
        <button>Enregistrer</button>
        {{ form_end(plageHoraireForm) }}
    {% endblock %}




